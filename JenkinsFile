pipeline {
    agent any
    stages {
        stage('Clone') {
            steps {
                git branch:'main',url:'https://github.com/Katsuo18/Jenkins'
            }
        }
        stage('Install') {
            steps {
                echo 'Instalando' 
            }
        }
        stage('Test')
        {
            steps {
                echo 'Testing'
            }
        }
    }
    post {
         always {
            echo 'Enviando Mensaje a mmunozc40@iescastelar.com'
        }
      }
}
